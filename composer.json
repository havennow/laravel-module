{
  "name": "havennow/laravel-module",
  "description": "Laravel light module",
  "keywords": [
    "havennow",
    "laravel",
    "laravel-module"
  ],
  "homepage": "https://github.com/havennow/laravel-module",
  "license": "MIT",
  "authors": [
    {
      "name": "Guilherme Trevisan de Oliveira",
      "email": "havennow@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.2.5|^8.0",
    "laravel/framework": "^6.0|^7.0|^8.0|^9.0|^10.0",
    "illuminate/contracts": "^6.0|^7.0|^8.0|^9.0|^10.0"
  },
  "require-dev": {
    "laravel/pint": "^1.0",
    "phpstan/phpstan": "^1.10",
    "phpunit/phpunit": "^8.5.8|^9.3.3"
  },
  "autoload": {
    "psr-4": {
      "Havennow\\LaravelModule\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Havennow\\LaravelModule\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Havennow\\LaravelModule\\ModuleServiceProvider"
      ],
      "aliases": {
        "Module": "Havennow\\LaravelModule\\Facades\\Module"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
